<?xml version="1.0"?>
<package>
  <metadata>
    <id>ReSharper.LocateFileInSolutionExplorer</id>
    <version>$version$</version>
    <authors>Igal Tabachnik</authors>
    <owners>Igal Tabachnik</owners>
    <title>Locate file or folder in Solution Explorer</title>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>https://raw.github.com/hmemcpy/ReSharper.LocateFileInSolutionExplorer/master/license.txt</licenseUrl>
    <description>Locate any file or folder by name in the solution explorer, without opening it. Invoked via double Shift-Alt-L.</description>
    <projectUrl>https://github.com/hmemcpy/ReSharper.LocateFileInSolutionExplorer</projectUrl>
    <iconUrl>https://raw.github.com/hmemcpy/ReSharper.LocateFileInSolutionExplorer/master/logo.png</iconUrl>
    <copyright>Copyright &#x00A9; 2013 Igal Tabachnik</copyright>
    <tags>locate find solution explorer</tags>
    <releaseNotes>Improved double Shift-Alt-L behavior, contributed by the amazing @controlflow!</releaseNotes>
    <dependencies>
      <dependency id="Resharper" version="[8.1,8.3)"/>
    </dependencies>
  </metadata>
  <files>
    <file src="..\output\$configuration$\8.1\*.*" target="ReSharper\v8.1\plugins"/>
    <file src="..\output\$configuration$\8.2\*.*" target="ReSharper\v8.2\plugins"/>
  </files>
</package>